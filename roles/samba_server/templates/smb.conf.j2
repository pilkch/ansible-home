[global]
	workgroup = WORKGROUP
	security = user

	passdb backend = tdbsam

	printing = cups
	printcap name = cups
	load printers = yes
	cups options = raw

	client min protocol = SMB3
	client max protocol = SMB3

[data1]
	path = {{ samba_folder }}
	writeable = yes
	browseable = yes
	guest ok = yes
	guest only = no
	create mask = 0777
	directory mode = 0777
	valid users = {{ samba_user }}
	force user = {{ samba_user }}
